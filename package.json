{
  "name": "logpilot-lite",
  "version": "0.0.3",
  "private": true,
  "workspaces": [
    "logpilot-rest-server",
    "logpilot-grpc-server"
  ],
  "main": "./dist/cjs/start-server.js",
  "module": "./dist/esm/start-server.mjs",
  "types": "./dist/types/start-server.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/start-server.js",
      "import": "./dist/esm/start-server.mjs",
      "types": "./dist/types/start-server.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:fix-extensions",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:fix-extensions": "node scripts/fix-extensions.js",
    "start": "ts-node --require tsconfig-paths/register start-server.ts",
    "dev:rest": "ts-node-dev --require tsconfig-paths/register logpilot-rest-server/src/server.ts",
    "dev:grpc": "ts-node-dev --require tsconfig-paths/register logpilot-grpc-server/src/server.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "grpc-tools": "^1.13.0",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^2.7.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
